{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "loading.html",
      "use": "@vercel/static"
    },
    {
      "src": "main.html",
      "use": "@vercel/static"
    },
    {
      "src": "main2.html",
      "use": "@vercel/static"
    },
    {
      "src": "book.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 
